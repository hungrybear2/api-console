{
    "$schema": "http://json-schema.org/schema",
    "properties": {
        "type": "array",
        "required": "true",
        "items": {
            "properties": {
                "categoryName": {
                    "type": "string",
                    "required": false
                },
                "comment": {
                    "type": "string",
                    "required": false
                },
                "commented": {
                    "type": "boolean",
                    "required": false
                },
                "id": {
                    "type": "number",
                    "required": false
                },
                "mealDescription": {
                    "type": "string",
                    "required": false
                },
                "mealId": {
                    "type": "number",
                    "required": false
                },
                "mealName": {
                    "type": "string",
                    "required": false
                },
                "options": {
                    "type": "array",
                    "required": false,
                    "items": {
                        "type": "string",
                        "required": false
                    }
                },
                "ratedWithinOrderFeedbackForm": {
                    "type": "boolean",
                    "required": false
                },
                "rated": {
                    "type": "boolean",
                    "required": false
                },
                "rating": {
                    "type": "string",
                    "required": false,
                    "enum": [
                        "",
                        "WOULD_ORDER_AGAIN",
                        "MAYBE_ORDER_AGAIN",
                        "WOULD_NOT_ORDER_AGAIN"
                    ]
                }
            }
        }
    }
}